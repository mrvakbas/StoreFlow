@model List<Todo>
@{
	ViewData["Title"] = "TodoAggreagatePriority";
	Layout = "~/Views/Layout/Index.cshtml";
}

<div class="row">

	<div class="col-lg-6 grid-margin stretch-card">
		<div class="card">
			<div class="card-body">
				<h5>Birincil Todo Listesi</h5>
				<br />
				<ul class="solid-bullet-list">

					@foreach (var desc in ViewBag.ListOfDescription as List<string>)
					{
						<li>@desc</li>
					}
				</ul>

				<p>Tüm Açıklamalar: @ViewBag.ResultsString</p>
			</div>
		</div>
	</div>

	<div class="col-lg-6 grid-margin stretch-card">
		<div class="card">
